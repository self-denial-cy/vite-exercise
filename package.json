{
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "commit": "cz",
    "release": "release-it",
    "prepare": "husky install",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint . --ext .vue,.ts,.tsx",
    "stylelintrc": "stylelint \"src/**/*.{vue,sass,scss,less,stylus,css}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/self-denial-cy/vite-exercise.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/self-denial-cy/vite-exercise/issues"
  },
  "homepage": "https://github.com/self-denial-cy/vite-exercise#readme",
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "gitlab": {
      "release": false
    },
    "github": {
      "release": false
    },
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.6",
    "postcss-styl": "^0.12.3",
    "prettier": "^2.8.7",
    "release-it": "^15.10.1",
    "sass": "^1.62.1",
    "stylelint": "^15.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "terser": "^5.17.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-chunk-split": "^0.4.7",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-inspect": "^0.7.26",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.4.2"
  }
}
